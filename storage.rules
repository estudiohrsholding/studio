rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow authorized admins to write to their club's member_ids folder
    match /clubs/{clubId}/member_ids/{fileName} {
      allow write: if request.auth != null && request.auth.token.clubId == clubId;
    }

    // Public read access for all files is generally acceptable if URLs are unguessable
    allow read;
  }
}